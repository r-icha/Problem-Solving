import java.util.*;
import java.util.Arrays;
public class BigArr{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] ar = new int[n];
        long sum = 0L;
        for(int i=0; i<n; i++){
            ar[i] = sc.nextInt();
            sum += ar[i];
        }
        System.out.print(sum);
        
    }
}
